 If pSpeaking(cyc)=0 And pMouth(cyc)>0
  pMouth(cyc)=0
  EntityTexture pLimb(cyc,1),tMouth(0),0,4
 EndIf
End Function

;FORM TEAM
Function FormTeam(char,v)
 If game=1 Or screen<>50
  ChangeRelationship(char,v,1) 
  charPartner(char)=v : charPartner(v)=char
  If Right$(charTeamName$(char),7)<>"'s Team" Then charTeamName$(v)=charTeamName$(char)
  If Right$(charTeamName$(v),7)<>"'s Team" Then charTeamName$(char)=charTeamName$(v) 
  If Right$(charTeamName$(v),7)="'s Team" Then charTeamName$(v)=textTeamName$(Rnd(1,no_teamnames)) : charTeamName$(char)=charTeamName$(v)
  If screen=50 Then ChangeAllegiance(char,charHeel(v))
 EndIf
End Function

;CHANGE RELATIONSHIP
Function ChangeRelationship(char,v,relation)
 If game=1 Or screen<>50
  ;change relationship
  charRelationship(char,v)=relation
  charRelationship(v,char)=relation
  ;affect other relationships
  If relation<0
   If charPartner(char)=v Then charPartner(char)=0
   If charPartner(v)=char Then charPartner(v)=0
   If charManager(char)=v Then charManager(char)=0
   If charManager(v)=char Then charManager(v)=0
  EndIf
 EndIf
End Function

;CHANGE REAL RELATIONSHIP
Function ChangeRealRelationship(char,v,relation)
 If game=1 Or screen<>50
  charRealRelationship(char,v)=relation
  charRealRelationship(v,char)=relation
 EndIf
End Function

;CHANGE ALLEGIANCE
Function ChangeAllegiance(char,allegiance)
 If screen=50
  cyc=FindCharacter(char)
  If pHeel(cyc)<>allegiance
   pHeel(cyc)=allegiance
   If pHeel(cyc)=0 Then charEyes(char)=charEyes(char)+1
   If pHeel(cyc)=1 Then charEyes(char)=charEyes(char)-1
  EndIf
 Else
  If charHeel(char)<>allegiance
   charHeel(char)=allegiance
   If charHeel(char)=0 Then charEyes(char)=charEyes(char)+1
   If charHeel(char)=1 Then charEyes(char)=charEyes(char)-1
  EndIf
 EndIf
 If charEyes(char)<1 Then charEyes(char)=1
 If charEyes(char)>3 Then charEyes(char)=3
End Function

;FIND FRIEND
Function FindFriend(char,task) ;-1=random, 0=oldest, 1=latest
 friend=0
 If task=>0
  If task=1 Then hi=9999999 Else hi=0
  For count=1 To fedSize(charFed(char))
   v=fedRoster(charFed(char),count)
   If task=0 And char<>v And charRelationship(char,v)>hi Then friend=v : hi=charRelationship(char,v) ;oldest
   If task=1 And char<>v And charRelationship(char,v)>0 And charRelationship(char,v)<hi Then friend=v : hi=charRelationship(char,v) ;latest
  Next
 EndIf
 ;random
 If task=-1
  its=0
  Repeat
   friend=fedRoster(charFed(char),Rnd(1,fedSize(charFed(char))))
   its=its+1
  Until (friend<>char And charRelationship(char,v)>0) Or its>1000
 EndIf
 Return friend
End Function

;FIND REAL FRIEND
Function FindRealFriend(char,task) ;-1=random, 0=oldest, 1=latest
 friend=0
 If task=>0
  If task=1 Then hi=9999999 Else hi=0
  For count=1 To fedSize(charFed(char))
   v=fedRoster(charFed(char),count)
   If task=0 And char<>v And charRealRelationship(char,v)>hi Then friend=v : hi=charRealRelationship(char,v) ;oldest
   If task=1 And char<>v And charRealRelationship(char,v)>0 And charRealRelationship(char,v)<hi Then friend=v : hi=charRealRelationship(char,v) ;latest
  Next
 EndIf
 ;random
 If task=-1
  its=0
  Repeat
   friend=fedRoster(charFed(char),Rnd(1,fedSize(charFed(char))))
   its=its+1
  Until (friend<>char And charRealRelationship(char,v)>0) Or its>1000
 EndIf
 Return friend
End Function

