;RISK HEEL/FACE TURNS
Function PushTurn(cyc,v) ;>0=circumstantial, 0=guaranteed face, -1=guaranteed heel
 If game=1
  If v>0
   ;heel vs heel results in face
   If charHeel(pChar(cyc))=1 And charHeel(pChar(v))=1 And pHeel(v)=1 Then pHeel(v)=0 : charEyes(pChar(cyc))=charEyes(pChar(cyc))+1
   ;face vs face results in heel
   If charHeel(pChar(cyc))=0 And charHeel(pChar(v))=0 And pHeel(cyc)=0 Then pHeel(cyc)=1 : charEyes(pChar(cyc))=charEyes(pChar(cyc))-1
  EndIf
  ;guaranteed face
  If v=0 And charHeel(pChar(cyc))=1 And pHeel(cyc)=1 Then pHeel(cyc)=0 : charEyes(pChar(cyc))=charEyes(pChar(cyc))+1
  ;guaranteed heel
  If v=-1 And charHeel(pChar(cyc))=0 And pHeel(cyc)=0 Then pHeel(cyc)=1 : charEyes(pChar(cyc))=charEyes(pChar(cyc))-1
 EndIf
 ;eye limits
 If charEyes(pChar(cyc))<1 Then charEyes(pChar(cyc))=1
 If charEyes(pChar(cyc))>3 Then charEyes(pChar(cyc))=3
End Function

;FIND STRENGTH
Function FindStrength(char,v)
 value=-1 : its=0
 Repeat
  its=its+1
  randy=Rnd(0,13)
  If (randy=0 And its>20) Or its>1000 Then value=0
  If InjuryStatus(char)=0 And charHealth(char)>charHealth(v)/2
   If randy=1 And charStrength(char)=>charStrength(v)+5 Then value=1
   If randy=2 And charSkill(char)=>charSkill(v)+5 Then value=2
   If randy=3 And charAgility(char)=>charAgility(v)+5 Then value=3
   If randy=4 And charStamina(char)=>charStamina(v)+5 Then value=4
   If randy=5 And charToughness(char)=>charToughness(v)+5 Then value=5
   If randy=6 And charAttitude(char)=>charAttitude(v)+5 Then value=14
   If charStrength(char)>charStrength(v) And charSkill(char)>charSkill(v) And charAgility(char)>charAgility(v) And charStamina(char)>charStamina(v) And charToughness(char)>charToughness(v)
    If randy=<6 Then value=6 
   EndIf
  EndIf
  If (randy=7 And charHealth(char)>charHealth(v)+(charHealth(v)/4) And charHealth(v)>50) Or (InjuryStatus(char)=0 And InjuryStatus(v)>0)
   value=13
  EndIf 
  If randy=8 And charPopularity(char)=>charPopularity(v)+5 Then value=7
  If randy=9 And (charHeight(char)=>charHeight(v)+5 Or TranslateWeight(char)>TranslateWeight(v)+(TranslateWeight(v)/2)) Then value=8
  If randy=10 And CountMatches(char,0)>4 And GetWinRate(char,0)>GetWinRate(v,0)+5 And GetWinRate(char,0)=>75 Then value=9
  If charManager(char)>0 Or charPartner(char)>0
   If randy=11 And charPartner(v)=0 And charManager(v)=0 Then value=10
  EndIf
  If randy=12 And charAge(char)=>charAge(v)+10 And charAge(v)<40 And CountMatches(char,0)>CountMatches(v,0) Then value=11
  If randy=13 And charAge(char)=<charAge(v)-10 And charAge(char)<40 And CountMatches(char,0)<CountMatches(v,0) Then value=12 
 Until value=>0 
 Return value
End Function

