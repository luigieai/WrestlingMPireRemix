;PREPARE PROMO
Function PreparePromo()
 ;risk shoots
 If game=1 And matchPromo>0 And ImportantPromo(matchPromo)=0 And gamSchedule(gamDate)=>1 And gamSchedule(gamDate)=<2
  char=pChar(1)
  randy=Rnd(0,(charAttitude(char)-20)*2)
  If randy=0 And charHappiness(char)<60 Then matchPromo=47 ;diss company
  If randy=1 And charRealRelationship(char,fedBooker(fed))<0 Then matchPromo=47 ;diss booker
  ;call out external enemy
  enemy=0 : its=0
  Repeat
   enemy=Rnd(1,no_chars) : its=its+1
   If its>1000 Then enemy=0
  Until (charRealRelationship(char,enemy)<0 And charFed(enemy)<>charFed(char) And charFed(enemy)=<8 And FindCharacter(enemy)=0) Or enemy=0
  If randy=2 And enemy>0 Then matchPromo=48 
  ;praise external friend
  friend=0 : its=0
  Repeat
   friend=Rnd(1,no_chars) : its=its+1
   If its>1000 Then friend=0
  Until (charRealRelationship(char,friend)>0 And charFed(friend)<>charFed(char) And charFed(friend)=<8 And FindCharacter(friend)=0) Or friend=0
  If randy=3 And friend>0 Then matchPromo=49 
 EndIf
 ;reset status
 promoLocked(matchPromo)=0
 promoTim=promoLength(matchPromo)
 promoLogic=1 : promoMesser=0
 promoVariable=0
 For count=1 To 20
  promoReact(count)=0
 Next
 ;reset actors
 For cyc=1 To 3
  promoActor(cyc)=0
 Next
 ;find key characters
 star=1
 If matchTeams>0 Then enemy=(no_wrestlers/2)+1 Else enemy=2
 ;assign actors
 If matchPromo>0
  For cyc=1 To 3
   ;prioritize stars
   If promoRole(matchPromo,cyc)=1 And FindActor(star)=0 Then promoActor(cyc)=star
   If promoRole(matchPromo,cyc)=2 And FindActor(enemy)=0 Then promoActor(cyc)=enemy
   ;involve team-mates
   If promoRole(matchPromo,cyc)>0
    For v=1 To no_plays
     If promoActor(cyc)=0 And FindActor(v)=0
      If promoRole(matchPromo,cyc)=1 And pTeam(v)=pTeam(star) Then promoActor(cyc)=v
      If promoRole(matchPromo,cyc)=2 And pTeam(v)=pTeam(enemy) Then promoActor(cyc)=v
     EndIf
    Next
    If promoActor(cyc)=0 Then promoActor(cyc)=promoRole(matchPromo,cyc)
   EndIf
   ;referee/secondary
   If promoRole(matchPromo,cyc)=-1
    For v=1 To no_plays
     If promoActor(cyc)=0 And pRole(v)=1 And FindActor(v)=0 Then promoActor(cyc)=v
    Next
    If promoActor(cyc)=0 Then promoActor(cyc)=no_wrestlers+no_refs
   EndIf
  Next
 EndIf
 ;get talking points
 If matchPromo=94
  promoSubTopicA=FindStrength(pChar(promoActor(2)),pChar(promoActor(1)))
  promoSubTopicB=FindStrength(pChar(promoActor(1)),pChar(promoActor(2)))
 EndIf
End Function

