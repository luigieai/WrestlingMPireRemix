;MAKE CHARACTER SPEAK
Function Speak(cyc,target,expression)
 ;switch focus
 camFoc=cyc : camType=10
 If target<>cyc And target=<no_plays Then pFoc(cyc)=target : pFoc(target)=cyc
 For v=1 To no_plays
  If v<>cyc And pTeam(v)<>pTeam(cyc) And pOutTim(v)>0 And pHidden(v)=0 And pControl(v)=0 Then pFoc(v)=cyc 
 Next
 ;expression
 pSpeaking(cyc)=1 : speaker=cyc
 If expression>0 Then pEyes(cyc)=expression
 ;background
 If screen<>50
  Color 0,0,0
  Rect rX#(0),rY#(480),rX#(800),rY#(600),1
 EndIf
End Function

;RESET EXPRESSIONS
Function ResetExpressions(cyc) 
 pOldEyes(cyc)=pEyes(cyc)
 If matchState<>2
  pEyes(cyc)=charEyes(pChar(cyc))
  If pFoc(cyc)>0
   If charRelationship(pChar(cyc),pChar(pFoc(cyc)))<0 Or pAngerTim(cyc,pFoc(cyc))>0 Then pEyes(cyc)=pEyes(cyc)-1
   If charRelationship(pChar(cyc),pChar(pFoc(cyc)))>0 And pAngerTim(cyc,pFoc(cyc))=0 Then pEyes(cyc)=pEyes(cyc)+1
  EndIf
 EndIf
