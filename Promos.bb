;//////////////////////////////////////////////////////////////////////////////
;------------------------------ DISPLAY PROMO ---------------------------------
;//////////////////////////////////////////////////////////////////////////////
Function DisplayPromo()
 ;reset speaking status
 speaker=0
 lineA$="" : lineB$=""
 redLineA$="" : redLineB$=""
 greenLineA$="" : greenLineB$=""
 charName$(0)="nobody"
 ;introduce widescreen
 factor#=100
 If promoTim=<25 Then factor#=GetPercent#(promoTim,25)
 If matchPromo=>0
  If promoTim=>promoLength(matchPromo)-25 Then factor#=GetPercent#(promoLength(matchPromo)-promoTim,25)
 Else
  If promoTim>25 Then factor#=100
 EndIf
 Color 0,0,0 : Rect rX#(0),rY#(0),rX#(800),rY#(PercentOf#(60,factor#)),1
 Color 0,0,0 : Rect rX#(0),rY#(600)-rY#(PercentOf#(120,factor#)),rX#(800),rY#(600),1
 ;~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -1. SCREW UP ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
 If matchPromo=-1
  If promoTim>25 And promoTim<275
   Speak(promoMesser,0,1)
   lineA$="Er... I... uh... DAMN! I forgot my line! SCREW IT!"
   lineB$="It was a stupid promo anyway! Just start the match..."
   If promoTim<200 And promoReact(10)=0 Then Pop(0,8,1) : Pop(0,7,0) : promoReact(10)=1
  EndIf
 EndIf
 ;~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ 1. WHY D'YOU INTERFER?! ~~~~~~~~~~~~~~~~~~~~~~~~~~
 If matchPromo=1
  If promoTim=>promoLength(matchPromo)-25 Then camFoc=promoActor(2) : camType=10
  If promoTim>850 And promoTim<1100
   Speak(promoActor(2),promoActor(1),1)
   lineA$="What the hell are you up to, "+charName$(pChar(promoActor(1)))+"?!"
   lineB$="Who do you think you are, interfering in MY business?!"
   If promoTim<975 And promoReact(1)=0 Then Pop(0,7,0) : promoReact(1)=1 : PromoCharm()
  EndIf
  If promoTim>575 And promoTim<825
   Speak(promoActor(2),promoActor(1),1) : PushTurn(promoActor(2),promoActor(1))
   lineA$="A guy that messes with my career is either incredibly"
   lineB$="brave or incredibly STUPID! Which one are you?!"
   If promoTim<700 And promoReact(2)=0 Then Pop(speaker,8,0) : promoReact(2)=1 : PromoCharm()
  EndIf
  If promoTim>300 And promoTim<550
   Speak(promoActor(1),promoActor(2),1)
   lineA$="Hey, I was just evening up the score! You've been"
   lineB$="stepping out line all too often in recent weeks..."
   If promoTim<425 And promoReact(3)=0 Then Pop(speaker,4,0) : promoReact(3)=1 : PromoCharm()
  EndIf
  If promoTim>25 And promoTim<275
   Speak(promoActor(2),promoActor(1),1)
   lineA$="Yeah, I make people's lives a misery all right - and you"
   lineB$="just added yourself to the list! Watch your back..."
   If promoTim<150 And promoReact(4)=0 Then Pop(speaker,2,0) : promoReact(4)=1 : PromoCharm()
   ChangeRelationship(pChar(promoActor(1)),pChar(promoActor(2)),-1)
  EndIf
 EndIf
 ;~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ 2. GLAD I INTERFERED! ~~~~~~~~~~~~~~~~~~~~~~~~~~
 If matchPromo=2
  If promoTim=>promoLength(matchPromo)-25 Then camFoc=promoActor(2) : camType=10
  If promoTim>850 And promoTim<1100
   Speak(promoActor(2),promoActor(1),2) : PushTurn(promoActor(2),promoActor(1))
   lineA$="Hey, "+charName$(pChar(promoActor(1)))+", I guess you're wondering"
   lineB$="why I got involved in your match last week?"
   If promoTim<950 And promoReact(1)=0 Then Pop(speaker,2,0) : promoReact(1)=1 : PromoCharm()
  EndIf
  If promoTim>575 And promoTim<825
   Speak(promoActor(1),promoActor(2),1)
   lineA$="To be honest, I don't care what you're up to."
   lineB$="I'm just glad I get to deliver the punishment!"
   If promoTim<750 And promoReact(2)=0 Then Pop(speaker,8,0) : promoReact(2)=1 : PromoCharm()
  EndIf
  If promoTim>300 And promoTim<550
   Speak(promoActor(2),promoActor(1),1)
   lineA$="I bring a bit of class to your match by showing my"
   lineB$="face - and THAT's the thanks I get?! You ingrate!"
   If promoTim<435 And promoReact(3)=0 Then Pop(speaker,2,0) : promoReact(3)=1 : PromoCharm()
  EndIf
  If promoTim>25 And promoTim<275
   Speak(promoActor(2),promoActor(1),1)
   lineA$="I guess I didn't make a good enough impression the"
   lineB$="first time, but I'm gonna make a BIG impact right now!"
   If promoTim<175 And promoReact(4)=0 Then Pop(0,6,0) : promoReact(4)=1 : PromoCharm()
   ChangeRelationship(pChar(promoActor(1)),pChar(promoActor(2)),-1)
  EndIf
 EndIf
 ;~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ 3. TEAM FORMATION ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
 If matchPromo=3
  If matchTeams=<0 Then promoLogic=0
  If promoTim=>promoLength(matchPromo)-25 Then camFoc=promoActor(1) : camType=10
  If promoTim>850 And promoTim<1100
   Speak(promoActor(1),0,0)
   lineA$="Ladies and gentlemen, you have the good fortune"
   lineB$="to witness a great moment in history here tonight!"
   If promoTim<975 And promoReact(1)=0 Then Pop(0,7,0) : promoReact(1)=1 : PromoCharm()
  EndIf
  If promoTim>575 And promoTim<825
   Speak(promoActor(2),0,0) : PushTurn(promoActor(2),0-pHeel(promoActor(1)))
   lineA$="Me and "+charName$(pChar(promoActor(1)))+" were good enough on"
   lineB$="our own, but now we're tearing it up as a TEAM!"
   If promoTim<750 And promoReact(2)=0 Then Pop(speaker,2,0) : promoReact(2)=1 : PromoCharm()
   FormTeam(pChar(promoActor(1)),pChar(promoActor(2)))
  EndIf
  If promoTim>300 And promoTim<550
   Speak(promoActor(1),0,1)
   lineA$="That's right. All the other teams better watch out!"
   lineB$="Me and "+charName$(pChar(promoActor(2)))+" are raising the bar..."
   If promoTim<435 And promoReact(3)=0 Then Pop(speaker,10,0) : promoReact(3)=1 : PromoCharm()
  EndIf
  If promoTim>25 And promoTim<275
   Speak(promoActor(2),promoActor(1),-1)
   lineA$="No doubt. But actions speak louder than words, so"
   lineB$="let's show the whole world what we're talking about!"
   If promoTim<175 And promoReact(4)=0 Then Pop(0,6,0) : promoReact(4)=1 : PromoCharm()
  EndIf
 EndIf
 ;~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ 4. THE BEGINNING ~~~~~~~~~~~~~~~~~~~~~~~~~~
 If matchPromo=4
  If promoTim=>promoLength(matchPromo)-25 Then camFoc=promoActor(2) : camType=10
  If promoTim>1125 And promoTim<1375
   Speak(promoActor(2),promoActor(1),3)
   lineA$="So, "+charName$(pChar(promoActor(1)))+", you wanna be a wrestler?"
   lineB$="Well, I hope you know it's not an easy lifestyle..."
   If promoTim<1250 And promoReact(1)=0 Then Pop(speaker,4,0) : promoReact(1)=1 : PromoCharm()
  EndIf
  If promoTim>850 And promoTim<1100
   Speak(promoActor(2),promoActor(1),1)
   lineA$="Sure, the bigtime is glamorous - but this ain't it!"
   lineB$="You'll be lucky if you EVER see the bright lights..."
   If promoTim<975 And promoReact(2)=0 Then Pop(0,10,0) : promoReact(2)=1 : PromoCharm()
  EndIf
  If promoTim>575 And promoTim<825
   Speak(promoActor(2),promoActor(1),3)
   lineA$="Some guys have been wrestling here for years without"
   lineB$="a single contract offer! You've gotta realise that..."
   If promoTim<700 And promoReact(3)=0 Then Pop(0,7,0) : promoReact(3)=1 : PromoCharm()
  EndIf
  If promoTim>300 And promoTim<550
   Speak(promoActor(2),promoActor(1),3)
   lineA$="That said, we're here to put you on the right track."
   lineB$="It'll be a long and rocky road, but stick with it..."
   If promoTim<435 And promoReact(4)=0 Then Pop(0,9,0) : promoReact(4)=1 : PromoCharm()
  EndIf
  If promoTim>25 And promoTim<275
   Speak(promoActor(1),promoActor(2),-1)
   lineA$="Alright, I know the score. Enough of the horror"
   lineB$="stories! Let me show you what I can do in the ring..."
   If promoTim<150 And promoReact(5)=0 Then Pop(0,6,0) : promoReact(5)=1 : PromoCharm()
  EndIf
 EndIf
 ;~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ 5. HOSTILE WELCOME TO THE FED ~~~~~~~~~~~~~~~~~~~~~~~~~~
 If matchPromo=5
  ;If charOldVacant(pChar(promoActor(1)))=0 Then promoLogic=0
  If promoTim=>promoLength(matchPromo)-25 Then camFoc=promoActor(1) : camType=10
  If promoTim>850 And promoTim<1000
   Speak(promoActor(1),0,-1)
   lineA$="Finally, "+charName$(pChar(speaker))+" has come to"
   lineB$=fedName$(charFed(pChar(speaker)))+"!"
   If promoTim<910 And promoReact(1)=0 Then Pop(speaker,2,0) : promoReact(1)=1 : PromoCharm()
