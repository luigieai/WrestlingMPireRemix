   If promoTim<910 And promoReact(1)=0 Then Pop(speaker,9,0) : promoReact(1)=1 : PromoCharm()
  EndIf
  If promoTim>575 And promoTim<825
   Speak(promoActor(2),promoActor(1),2)
   lineA$="We've all heard a lot about you. Sounds like you've"
   lineB$="been causing quite a stir in this business?"
   If promoTim<750 And promoReact(2)=0 Then Pop(speaker,10,0) : promoReact(2)=1 : PromoCharm()
