   Speak(promoActor(2),promoActor(1),3)
   lineA$="I'm honoured to be the one that kicks off your time"
   lineB$="with us! Let's see what you're really made of..."
   If promoTim<435 And promoReact(3)=0 Then Pop(0,6,0) : promoReact(3)=1 : PromoCharm()
  EndIf
  If promoTim>25 And promoTim<275
   Speak(promoActor(1),promoActor(2),-1)
   lineA$="Thanks, "+charName$(pChar(promoActor(2)))+". It's good to be here."
   lineB$="You won't be so happy when I beat you though!"
   If promoTim<175 And promoReact(4)=0 Then Pop(speaker,2,0) : promoReact(4)=1 : PromoCharm()
   If pHeel(promoActor(1))=pHeel(promoActor(1))
    ChangeRelationship(pChar(promoActor(1)),pChar(promoActor(2)),1)
   Else
    ChangeRelationship(pChar(promoActor(1)),pChar(promoActor(2)),-1)
   EndIf
  EndIf
 EndIf
 ;~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ 7. NOBODY WANTS YOU HERE ~~~~~~~~~~~~~~~~~~~~~~~~~~
 If matchPromo=7
  If charContract(pChar(promoActor(1)))=0 Or charSalary(pChar(promoActor(1)))=0 Then promoLogic=0
  If promoTim=>promoLength(matchPromo)-25 Then camFoc=promoActor(2) : camType=10
  If promoTim>1400 And promoTim<1650
   Speak(promoActor(2),promoActor(1),1)
   lineA$="Hey, "+charName$(pChar(promoActor(1)))+", why do you bother turning"
   lineB$="up week after week?! NOBODY wants you here!"
   If promoTim<1520 And promoReact(1)=0 Then Pop(speaker,4,0) : promoReact(1)=1 : PromoCharm()
  EndIf
  If promoTim>1125 And promoTim<1375
   Speak(promoActor(2),promoActor(1),1) : PushTurn(promoActor(2),promoActor(1))
   lineA$="You drag your carcass into this ring, bore the"
   lineB$="crowd to tears - and then take home a cheque?!"
   If promoTim<1220 And promoReact(2)=0 Then Pop(speaker,2,0) : promoReact(2)=1 : PromoCharm()
  EndIf
  If promoTim>850 And promoTim<1100
   Speak(promoActor(2),promoActor(1),1)
   lineA$="Losers like you are a waste of the fans' money and"
   lineB$="the company's money! Do everyone a favour and leave..."
   If promoTim<950 And promoReact(3)=0 Then Pop(0,7,0) : promoReact(3)=1 : PromoCharm()
  EndIf
  If promoTim>575 And promoTim<825
   Speak(promoActor(1),promoActor(2),-1)
   lineA$="What are you talking about, "+charName$(pChar(promoActor(2)))+"?!"
   lineB$="I've got a legitimate contract like everybody else!"
   If promoTim<750 And promoReact(4)=0 Then Pop(speaker,2,0) : promoReact(4)=1 : PromoCharm()
  EndIf
  If promoTim>300 And promoTim<550
   Speak(promoActor(1),promoActor(2),-1)
   lineA$="I take it you're not happy with YOUR miserable career?!"
   lineB$="Whatever your problem is, don't take it out on me..."
   If promoTim<435 And promoReact(5)=0 Then Pop(speaker,8,0) : promoReact(5)=1 : PromoCharm()
  EndIf
  If promoTim>25 And promoTim<275
   Speak(promoActor(1),promoActor(2),1)
   lineA$="And as for 'boring the crowd', come here and I'll"
   lineB$="give them a massacre that they'll NEVER forget!"
   If promoTim<175 And promoReact(6)=0 Then Pop(0,6,0) : promoReact(6)=1 : PromoCharm()
   ChangeRelationship(pChar(promoActor(1)),pChar(promoActor(2)),-1)
